{
  "name": "periodicjs.ext.reactadmin",
  "description": "react based admin.",
  "version": "3.0.0",
  "main": "index.js",
  "engines": {
    "node": "^6.x"
  },
  "config": {
    "blanket": {
      "pattern": [
        "content/themes"
      ]
    }
